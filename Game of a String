#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int n, zero = 0;
        cin >> n;
        string s;
        cin >> s;

        for (int i = 0; i < n; i++)
        {
            if (s[i] == '0')
            {
                zero++;
            }
        }

        if (n == 2 && (s[0] || s[n - 1]))
        {
            cout << "ANIL" << endl;
        }
        else if (zero == 0 || s == "100001" || (s[0] == '0' && n > 1) || (s[n - 1] == '0' && n > 1))
        {
            cout << "DRAW" << endl;
        }
        else
        {
            cout << "SAMEER" << endl;
        }
    }
    return 0;
}
